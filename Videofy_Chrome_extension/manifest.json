{
  "manifest_version": 2,
  "name": "VIDEOFY OPENER",
  "description": "Extension to Add Code-Now Button to Problem Pages. One Button for each - C, C++, Java and Python is added.",
  "version": "1.0",
  "author": "PRIYANSHU VARSHNEY",
  "short_name" : "VIDEOFY",
  "content_scripts": [
   
    
    {
      "run_at" : "document_end",
      "matches" : ["https://www.wittyfeed.com/story/*"],
      "js" : ["jquery.js", "basic.js"]
    }
   
  ],

  "icons":{
    "16" : "videofy.png",
    "48" : "videofy.png",
    "128": "videofy.png"
  },
  "permissions" : [
    "tabs", "nativeMessaging", "https://www.wittyfeed.com/story/*"]}